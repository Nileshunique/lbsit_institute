{
  "name": "lbsit_institute",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://lbsitinstitute.com",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@heroicons/react": "^2.1.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=/ react-scripts build",
    "build:github": "PUBLIC_URL=/lbsit_institute react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false --passWithNoTests",
    "test:ci": "react-scripts test --coverage --watchAll=false --passWithNoTests",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md}",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.9.5"
  }
}
